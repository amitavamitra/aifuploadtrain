<%- include('partials/header'); -%>
<body>
 <h1>Load -> Register-> Configure & Execute Pipeline</h1>
    <form  action="/" method="post">
        <div class="form-row">
                  <div class="form-group row">
                    <label  class="col-sm-3 col-form-label"  for="bucket">s3-bucket</label>
                    <div class="col-sm-8">
                      <input type="text"  name="bucket" id="bucket" 
                      class="form-control col-sm-8 " placeholder="s3-bucket"
                      value="<%=bucket%>">
                    </div>
                  </div>
                  <div class="form-group row">
                    <label  class="col-sm-3 col-form-label"  for="access">s3-accessKey</label>
                    <div class="col-sm-8">
                    <input type="password" name="accesskey" id="access" 
                    class="form-control col-sm-8" placeholder="s3-accessKey"
                    value="<%=access_key_id%>">
                    </div>
                  </div>
                  <div class="form-group row">
                    <label  class="col-sm-3 col-form-label"  for="access">s3-secretKey</label>
                    <div class="col-sm-8">
                      <input type="password" name="secret" id="secret"  
                      class="form-control  col-sm-8" placeholder="s3-secret"
                      value="<%=secret_access_key%>">
                    </div>
                  </div>

                  <div class="form-group row">
                    <label  class="col-sm-3 col-form-label"  for="access">prefix</label>
                    <div class="col-sm-8">
                      <input type="text" class="form-control  col-sm-8"  name="prefix" id="prefix" placeholder="data"             
                    </div>
                  </div>

                  <br><br>
                   
                  <div class="form-group row">
                    <label  class="col-sm-3 col-form-label"  for="access">scenarioID</label>  
                    <div class="col-sm-8">
                      <select  class="form-control  col-sm-8"  name="scenario_id"   id="scenario_id">
                        </select>              
                    </div>
                  </div>

                  <div class="form-group row">
                    <label  class="col-sm-3 col-form-label"  for="access">name</label>  
                    <div class="col-sm-8">    
                        <input type="text" required="true" class="form-control" name="name" id="name" placeholder="name">
                    </div>
                  </div>
                    
                  
                  <div class="form-group row">
                    <label  class="col-sm-3 col-form-label"  for="access">executableId</label>  
                    <div class="col-sm-8">    
                         <input type="text" required="true"  class="form-control" name="executableId" id="executableId" placeholder="executableId">
                      </div>
                  </div>
                  
                  <div class="form-group row">
                    <label  class="col-sm-3 col-form-label"  for="access">description</label>  
                    <div class="col-sm-8">    
                     <input type="text" required="true" class="form-control" name="description" id="description" placeholder="A meaningful description">
                    </div>
                  </div>


                  <div class="form-group row">
                    <label  class="col-sm-3 col-form-label"  for="access">training data</label>  
                    <div class="col-sm-8">   
                      <input id="input-5" name="input5[]" type="file" class="file"  data-show-upload="false" data-show-caption="true" multiple> 
                     
                    </div>
                  </div>


            <div class="col">
                <button class="btn btn-primary" type="submit">Submit</button>      
             </div>
    </div>
    
    
    </form>
          </div>
          <br><br><br>
        <hr>
          <div class="form-row">
            <div class="col">
            <input type="text" class="form-control" name="artifact" id="artifact" placeholder="The Artifact Registered" value="<%=new_artifact.id%>">
            </div>
            <div class="col">
            <input type="text" class="form-control" name="configuration" id="configuration" placeholder="The Configuration Created" value="<%=new_config.id%>">
            </div>
            <div class="col">
